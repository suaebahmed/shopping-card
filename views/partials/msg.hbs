
{{!-- login / --}}
{{#if error}}
    <div class="alert alert-danger">
        <h6>{{error}}</h6>
    </div>
{{/if}}

{{!-- signup / --}}
{{#if success}}
    <div class="alert alert-success">{{success}}</div>
{{/if}}

{{#if signupValidErr}}
    {{#each signupValidErr}}
        <div class="alert alert-danger">{{this.msg}}</div>
    {{/each}}
{{/if}}

