
{{!-- login / --}}
{{#if error}}
    <h1>{{error}}</h1>
    <p>this err msg</p>
{{/if}}

{{!-- signup / --}}
{{#if success}}
    <div class="alert alert-success">{{success}}</div>
{{/if}}

{{#if signupValidErr}}
    {{#each signupValidErr}}
        <div class="alert alert-danger">{{this.msg}}</div>
    {{/each}}
{{/if}}

